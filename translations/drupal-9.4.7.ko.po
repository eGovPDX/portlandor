msgstr "너비: @data.width"
msgid "height @data.height"
msgstr "높이: @data.height"
msgid "Administer workflows"
msgstr "워크플로우 관리"
msgid "Content type where this field is in use."
msgstr "이 필드가 사용 중인 콘텐츠 유형입니다."
