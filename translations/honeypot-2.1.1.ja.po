msgid "Preview"
msgstr "プレビュー"
msgid "Timestamp"
msgstr "タイムスタンプ"
msgid "Expiration"
msgstr "有効期限"
msgid "seconds"
msgstr "秒"
msgid "Spam control"
msgstr "スパムコントロール"
