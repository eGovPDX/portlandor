{% embed "block.html.twig" with {
  block_classes: [
    'd-block',
    'd-lg-flex',
    'align-items-center',
  ],
  content_classes: [
    'content',
    'w-100',
  ]
} %}
{% block content %}
  <div{{ content_attributes.addClass(content_classes) }}>
    {{ content|set_attribute('role', 'search') }}
  </div>
{% endblock %}
{% endembed %}
