{% embed 'field.html.twig' with {
  'margin' : '4'
  } %}
  {% block content %}
    {% apply spaceless %}
      {%- for item in items -%}
        {% if item.content|render|striptags|trim is not empty %}
        {% set username = item.content["#context"].value %}
        <a href="tel:711">
          {% include '@elements/icon/icon.twig' with {
            name: 'oregon-relay'
          } only %}
          711
        </a>
        {% endif %}
      {%- endfor -%}
    {% endapply %}
  {% endblock %}
{% endembed %}
