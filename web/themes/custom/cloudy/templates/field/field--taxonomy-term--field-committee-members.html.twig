{# Hide chair and vice chair from member list so they aren't duplicated #}
{% set chair_gids = element['#object'].field_chair|map(item => item.target_id) %}
{% set vice_chair_gid = element['#object'].field_vice_chair.target_id %}
{% include "field.html.twig" with {
  items: items|filter(item => item.content['#group'].id() not in chair_gids and item.content['#group'].id() != vice_chair_gid),
} %}
