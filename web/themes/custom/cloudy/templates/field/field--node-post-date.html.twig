{% embed 'field.html.twig' with {
    'margin' : '4',
} %}
  {% spaceless %}
    {% block content %}
      {%- for item in items if item.content|render|striptags|trim is not empty -%}
        {% set item_classes = item_classes|merge(['time']) %}
        {% set post_date = item.content.value|date('M j, Y') %}
        <time{{item.attributes.addClass(item_classes)}}>
          {{ post_date }}
        </time>
      {%- endfor -%}
    {% endblock %}
  {% endspaceless %}
{% endembed %}
