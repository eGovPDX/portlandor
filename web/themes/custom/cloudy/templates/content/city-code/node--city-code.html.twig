<article{{ attributes }}>
  <div{{ content_attributes.addClass('node__content', 'clearfix') }}>
    {{ content }}
  </div>
</article>
{% set view_effected_on_codecharterpolicy = drupal_block('views_block:change_sets-effected_on_codecharterpolicy')|length %}
{% if view_effected_on_codecharterpolicy > 0 %}
  {{ drupal_block('views_block:change_sets-effected_on_codecharterpolicy') }}
{% endif %}
{% set view_news = drupal_block('views_block:news_on_content-block_2')|length %}
{% if view_news > 0 %}
  {{ drupal_block('views_block:news_on_content-block_2') }}
{% endif %}
{% set view_events = drupal_block('views_block:events_on_content-block_1')|length %}
{% if view_events > 0 %}
  {{ drupal_block('views_block:events_on_content-block_1') }}
{% endif %}