<article{{ attributes }}>
  <div{{ content_attributes.addClass('node__content', 'clearfix') }}>
    {{ content }}
  </div>
</article>
{% set view_effected_on_codecharterpolicy = drupal_block('views_block:change_sets-effected_on_codecharterpolicy') %}
{% if view_effected_on_codecharterpolicy|length > 0 %}
  {{ view_effected_on_codecharterpolicy }}
{% endif %}
{% set view_news = drupal_block('views_block:news_on_content-block_content_news') %}
{% if view_news|length > 0 %}
  {{ view_news }}
{% endif %}
{% set view_events = drupal_block('views_block:events_on_content-block_1') %}
{% if view_events|length > 0 %}
  {{ view_events }}
{% endif %}
