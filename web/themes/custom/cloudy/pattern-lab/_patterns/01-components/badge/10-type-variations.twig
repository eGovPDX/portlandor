{# Show all possible variations from changing a SINGLE variable #}

{% set types = ['popular', 'service'] %}

{% for type in types %}
  <div>
    <h6>Type: <code>{{ type }}</code></h6>
    {% include "@components/badge/badge.twig" with {
      "text": type,
      "type": type,
    } only %}
  </div>
{% endfor %}
