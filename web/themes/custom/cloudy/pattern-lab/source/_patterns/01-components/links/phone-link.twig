{% set icon %}
  {% include "@elements/icons/phone.twig" with {
    "classes": [
      "h-6",
      "w-6"
    ]
  } only %}
{% endset %}
{% set phone_number = phone_number|default('000-000-0000') %}
{% set url = "tel:+1" ~ phone_number %}
{% include "@components/links/_icon-link.twig" with {
  "icon": icon,
  "url": url,
  "text": phone_number
} only %}
