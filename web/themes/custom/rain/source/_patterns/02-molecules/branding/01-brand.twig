{#
/**
 * Available variables:
 * - brand_prefix
 * - brand_main
 * - brand_suffix
 * - url
 */
#}
{% set content %}
    <em>{{ brand_prefix }}</em>
    {% include "@atoms/02-text/00-headings/_heading.twig" with {
        "heading_level": 1,
        "heading": brand_main,
    } %}
    <em>{{ brand_suffix }}</em>
{% endset %}
{% include "@atoms/01-links/link/link.twig" with {
    "link_url": url,
    "link_content": content,
    "link_modifiers": ["site-brand"]
} %}
