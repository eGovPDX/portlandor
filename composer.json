    "conflict": {
        "drupal/drupal": "*",
        "doctrine/common": ">2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "process-timeout": 0
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php.",
            "This file specifies the packages.drupal.org repository.",
            "You can read more about this composer repository at:",
            "https://www.drupal.org/node/2718229"
        ],
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "enable-patching": true,
        "composer-exit-on-patch-failure": 1,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "2567035 - Database::startLog() cause a notice in Shortcut::sort()": "https://www.drupal.org/files/issues/2019-12-05/2567035-51.patch",
                "2797583 - Dynamically provide action plugins for every moderation state change": "https://www.drupal.org/files/issues/2019-04-28/2797583-102_8_7_x.patch",
                "1349080 - node_access filters out accessible nodes when node is left joined.": "https://www.drupal.org/files/issues/2019-11-13/1349080-429.patch",
                "2885278 - Node local tasks tabs do not appear on node revisions": "https://www.drupal.org/files/issues/2018-04-19/node-revisions-missing-local-tasks-2885278-11.patch",
                "2769289 - Revision User information not displayed in views when content revision user is selected for View type Content": "https://www.drupal.org/files/issues/2018-09-07/node_revision_author-2769289-11.patch",
                "2457999 - Cannot use relationship for rendered entity on Views": "https://www.drupal.org/files/issues/2020-06-17/cannot-use-relationship-2457999-244.patch",
                "2998826 - Term route context (allows block placed on term pages)": "https://www.drupal.org/files/issues/2019-04-12/term_route_context-2998826-31.patch"
            },
            "drupal/bootstrap_datepicker": {
                "POWR-904/854/1017 - Submodule for exposed filter with custom identifier": "patches/powr-904-bootstrap-datepicker.patch"
            },
            "drupal/ctools": {
                "2475595 - Views exposed filters missing autosubmit option https://www.drupal.org/node/2475595": "https://www.drupal.org/files/issues/2475595-ctools-autocomplete-fix-24.patch",
                "2785499 Entity Bundle Condition Plugin does not work in all situations": "https://www.drupal.org/files/issues/2019-03-01/ctools_fix_condition_plugin_entity_bundle-2785499-12.patch"
            },
            "drupal/content_lock": {
                "2919019 - content_lock_entity_operation doesn't work if the entity list is cached": "https://www.drupal.org/files/issues/content_lock-caching-invalidation-2919019-3.patch"
            },
            "drupal/group": {
                "2774827 - node:group functionality for adding node:group:title": "https://www.drupal.org/files/issues/group_add-node-group-tokens-2774827-22.patch",
                "2842630 - Empty page when trying to create group node": "https://www.drupal.org/files/issues/2018-09-13/2842630-15.patch",
                "POWR-1254 - Patch Group module to remove term node and make verbiage more user friendly": "patches/powr-1254-group-create-content.patch"
            },
            "drupal/groupmedia": {
                "POWR-833 - Patch Groupmedia module to make the verbiage consistent with Group/gnode, display media type descriptions": "patches/powr-833-groupmedia.patch"
            },
            "drupal/clamav": {
                "2915237 - allowUncheckedFiles() not working as expected": "https://www.drupal.org/files/issues/2915237-2.patch"
            },
            "drupal/search_api_pantheon": {
                "2996001 - Use Lando client cert": "https://www.drupal.org/files/issues/2018-08-29/use_lando_cert.patch"
            },
            "drupal/menu_token": {
                "2948260 - Tokens not substituted in query string": "https://www.drupal.org/files/issues/2018-09-21/2988939-allow-query-parameters-12.diff"
            },
            "drupal/menu_link": {
                "3019509 - Menu item without attributes causes a notice": "https://www.drupal.org/files/issues/2018-12-10/menu_link-attributes-error-3019509-3.patch"
            },
            "drupal/geocoder": {
                "Skip geocoding when address_line1 is empty": "patches/powr-1275-geocoder-empty-address.patch"
            },
            "drupal/leaflet": {
                "POWR-1352 Show multiple base layers at once": "patches/leaflet-multiple-base-layer.patch"
            },
            "drupal/conditional_fields": {
                "2983462 - Fix required-handling": "https://www.drupal.org/files/issues/2019-07-30/conditional_fields-3063330-11-custom-form-modes.patch"
            },
            "drupal/inline_entity_form": {
                "2875716 - Widget settings to control removing & deleting existing references [rerolled]": "patches/inline_entity_form_8.x-1.x_2875716_reroll.patch"
            },
            "drupal/openid_connect_windows_aad": {
                "Custom patch to use scope, and 3159055 - Option to use concatenation of givenName and surname instead of default name": "patches/powr2529-openid-connect-windows-aad.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library","type:bower-asset","type:npm-asset"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
